{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "UsageTracking": {
    "Enabled": true,
    "Providers": {
      "openai": {
        "Enabled": true,
        "EnableCostCalculation": false
      },
      "anthropic": {
        "Enabled": true,
        "EnableCostCalculation": true
      }
    }
  }
}
