{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information",
      "AIProxy.Proxy.Middleware": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://+:5501"
      },
      "Https": {
        "Url": "https://+:5502"
      }
    },
    "Certificates": {
      "Default": {
        "Path": "certs/certificate.crt",
        "KeyPath": "certs/private.key"
      }
    }
  }
}